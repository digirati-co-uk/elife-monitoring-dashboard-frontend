<section class="article-version-history">
    <ol class="article-version-history-list">
        {{#each currentEvents.events as |events|}}
            <li>
                <div class="column-1">
                    start (orange), end (green), error (red)
                    {{#is publication-status 'start'}}
                        <!--<span class="glyphicon glyphicon-ok {{event-status}} glyphicon-ok--mod pull-left"></span>-->
                    {{/is}}
                    {{#is publication-status 'end'}}
                        <!--<span class="glyphicon glyphicon-ok {{event-status}} glyphicon-ok--mod pull-left"></span>-->
                    {{/is}}
                    {{#is publication-status 'error'}}
                        <!--<span class="glyphicon glyphicon-warning {{event-status}} glyphicon-warning-sign--mod pull-left"></span>-->
                    {{/is}}

                    <span>{{event-type}}</span>
                </div>
                <div class="column-2">
                    <dl>
                        <dt><i>Timestamp:</i></dt>
                        <dd class="divide">{{elFormatUnixDate event-timestamp "MM/DD/YYYY" }}</dd>
                        <dd class="divide">{{elFormatUnixDate event-timestamp "HH:mm:ss" }}</dd>
                    </dl>
                    <dl>
                        <dt><i>Message:</i></dt>
                        <dd>{{event-message}}</dd>
                    </dl>
                </div>
            </li>
        {{/each}}
    </ol>
</section>